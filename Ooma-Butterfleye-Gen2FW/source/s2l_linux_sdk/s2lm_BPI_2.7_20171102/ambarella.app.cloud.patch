+    TU8 packet_len_7to0;
+} STSPrivateDataPesPacketHeader;
+
+typedef struct {
+    TU8 data_type_4cc_31to24;
+    TU8 data_type_4cc_23to16;
+    TU8 data_type_4cc_15to8;
+    TU8 data_type_4cc_7to0;
+    TU8 data_sub_type_15to8;
+    TU8 data_sub_type_7to0;
