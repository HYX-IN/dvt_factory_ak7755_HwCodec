+EECode CAmbaCloudAgent::readSimpleConfFile()
+{
+    EECode err = EECode_OK;
+    TChar read_string[256] = {0};
+    TInt read_len = 0;
+
+    IRunTimeConfigAPI *p_config_file = gfRunTimeConfigAPIFactory(ERunTimeConfigType_SimpleINI, 128);
+    if (DUnlikely(!p_config_file)) {
+        LOG_ERROR("gfRunTimeConfigAPIFactory(ERunTimeConfigType_SimpleINI) fail.\n");
+        return EECode_NoMemory;
+    }
+
+    err = p_config_file->OpenConfigFile(mpAccountConfFilename, 0, 0, 1);
+    if (DUnlikely(EECode_OK != err)) {
+        LOG_ERROR("OpenConfigFile(%s) fail, ret %d, %s.\n", mpAccountConfFilename, err, gfGetErrorCodeString(err));
+        return err;
+    }
+
+    err = p_config_file->GetContentValue("datasrvurl", (TChar *)read_string);
+    if (DUnlikely(EECode_OK != err)) {
+        LOG_ERROR("GetContentValue(datasrvurl) fail, ret %d, %s.\n", err, gfGetErrorCodeString(err));
+        p_config_file->CloseConfigFile();
+        return err;
+    } else {
+        mpCloudServerUrl = (char *) malloc(strlen(read_string) + 4);
+        if (mpCloudServerUrl) {
+            strcpy(mpCloudServerUrl, read_string);
+            LOG_NOTICE("[read configfile]: datasrvurl:%s, read_len=%d\n", mpCloudServerUrl, read_len);
+        } else {
+            LOG_FATAL("no memory\n");
+            return EECode_NoMemory;
+        }
+    }
+
+    err = p_config_file->GetContentValue("datasrvport", (TChar *)read_string);
+    if (DUnlikely(EECode_OK != err)) {
+        LOG_ERROR("GetContentValue(datasrvport) fail, ret %d, %s.\n", err, gfGetErrorCodeString(err));
+        p_config_file->CloseConfigFile();
+        return err;
+    } else {
+        read_len = sscanf(read_string, "%hu", &mCloudServerPort);
+        if (DUnlikely(1 != read_len)) {
+            LOG_ERROR("GetContentValue(datasrvport) fail, %s, read_len=%d.\n", read_string, read_len);
+            p_config_file->CloseConfigFile();
+            return err;
+        }
+        LOG_NOTICE("[read configfile]: datasrvport:%hu\n", mCloudServerPort);
+    }
+
+    err = p_config_file->GetContentValue("channel", (TChar *)read_string);
+    if (DUnlikely(EECode_OK != err)) {
+        LOG_ERROR("GetContentValue(channel) fail, ret %d, %s.\n", err, gfGetErrorCodeString(err));
+        err = p_config_file->CloseConfigFile();
+        return err;
+    } else {
+        read_len = snprintf(mDeviceAccountName, sizeof(mDeviceAccountName), "%s",  read_string);
+        LOG_NOTICE("[read configfile]: channel:%s\n", mDeviceAccountName);
+    }
+
+    p_config_file->CloseConfigFile();
+    p_config_file->Destroy();
+
+    return EECode_OK;
+}
